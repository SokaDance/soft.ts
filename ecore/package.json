{
  "name": "@masagroup/ecore",
  "version": "1.3.0",
  "description": "ecore typescript library",
  "author": "MASA Group",
  "license": "MPL-2.0",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "npm run build-types && npm run build-server && npm run build-browser",
    "build-browser": "tsc -p tsconfig.cjs.json",
    "build-types": "tsc -p tsconfig.types.json",
    "build-server": "tsc -p tsconfig.esm.json",
    "pretty": "prettier --write src/**/*.ts",
    "test": "jest"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.4",
    "@types/jest": "^29.5.12",
    "deep-equal": "^2.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-mockito": "^2.6.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@types/sax": "^1.2.7",
    "id128": "^1.6.6",
    "sax": "^1.4.1",
    "stream-to-array": "^2.3.0",
    "ts-results-es": "^4.2.0"
  }
}
